<!doctype html>
<html>
  <head>
    <title>Users</title>
    <link rel='stylesheet' href='css/main.css'>
  </head>
  <body>
    <div id='container'>
      <h2>Here are all the users</h2>
      <% for (var x in users) { %>
        <h3><a href='/users/<%= x %>'>Name: <%= users[x].name %></a></h3>
        <h4>Email: <%= users[x].email %></h4>
        <hr>
      <% } %>
    </div>
  </body>
</html>
